{% extends "admin/base.html.twig" %}
{% block title %}
    Ajouter une categorie
{% endblock title %}

{% block body %}
<div class="container">
    <h1>Ajouter un categorie</h1>
    <div class="col-8">

        {{ form_start(form) }}
        <div class="form-group ">
            {{ form_row(form.name) }}
        </div>
        <br>
        <button  class="btn btn-primary">Ajouter</button>
        {{ form_end(form) }}
    </div>
</div>
{% endblock body %}